{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "i386-pc-windows-gnu (PIC)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/loader.py",
            "args": [
                "--arch",
                "i386",
                "${workspaceFolder}/build/release/windows/i386/output.bin"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "i386-pc-windows-gnu-release"
        },
        {
            "name": "x86_64-pc-windows-gnu (PIC)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/loader.py",
            "args": [
                "--arch",
                "x86_64",
                "${workspaceFolder}/build/release/windows/x86_64/output.bin"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "x86_64-pc-windows-gnu-release"
        },
        {
            "name": "aarch64-pc-windows-gnu (PIC)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/loader.py",
            "args": [
                "--arch",
                "aarch64",
                "${workspaceFolder}/build/release/windows/aarch64/output.bin"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "aarch64-pc-windows-gnu-release"
        },
        {
            "name": "i386-pc-windows-gnu-debug",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/debug/windows/i386/output.exe",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "preLaunchTask": "i386-pc-windows-gnu-debug"
        },
        {
            "name": "x86_64-pc-windows-gnu-debug",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/debug/windows/x86_64/output.exe",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "preLaunchTask": "x86_64-pc-windows-gnu-debug"
        },
        {
            "name": "armv7a-pc-windows-gnu-debug",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/debug/windows/armv7a/output.exe",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "preLaunchTask": "armv7a-pc-windows-gnu-debug"
        },
        {
            "name": "aarch64-pc-windows-gnu-debug",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/debug/windows/aarch64/output.exe",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "preLaunchTask": "aarch64-pc-windows-gnu-debug"
        },
        {
            "name": "i386-unknown-linux-gnu-debug (WSL)",
            "type": "lldb-dap",
            "request": "launch",
            "program": "${workspaceFolder}/build/debug/linux/i386/output.elf",
            "cwd": "${workspaceFolder}",
            "internalConsoleOptions": "openOnSessionStart",
            "preLaunchTask": "i386-unknown-linux-gnu-debug"
        },
        {
            "name": "x86_64-unknown-linux-gnu-debug (WSL)",
            "type": "lldb-dap",
            "request": "launch",
            "program": "${workspaceFolder}/build/debug/linux/x86_64/output.elf",
            "cwd": "${workspaceFolder}",
            "internalConsoleOptions": "openOnSessionStart",
            "preLaunchTask": "x86_64-unknown-linux-gnu-debug"
        },
        {
            "name": "armv7a-unknown-linux-gnueabihf-debug (WSL)",
            "type": "lldb-dap",
            "request": "launch",
            "program": "${workspaceFolder}/build/debug/linux/armv7a/output.elf",
            "cwd": "${workspaceFolder}",
            "internalConsoleOptions": "openOnSessionStart",
            "preLaunchTask": "armv7a-unknown-linux-gnueabihf-debug"
        },
        {
            "name": "aarch64-unknown-linux-gnu-debug (WSL)",
            "type": "lldb-dap",
            "request": "launch",
            "program": "${workspaceFolder}/build/debug/linux/aarch64/output.elf",
            "cwd": "${workspaceFolder}",
            "internalConsoleOptions": "openOnSessionStart",
            "preLaunchTask": "aarch64-unknown-linux-gnu-debug"
        }
    ]
}