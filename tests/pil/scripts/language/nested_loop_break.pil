var count = 0;
for (var i in [1, 2, 3]) {
    for (var j in [1, 2, 3, 4, 5]) {
        if (j > 2) {
            break;
        }
        count = count + 1;
    }
}
print("Nested loop count (3 outer * 2 inner):", count);

// Break only affects innermost loop
var outer = 0;
var inner = 0;
var x = 0;
while (x < 3) {
    outer = outer + 1;
    var y = 0;
    while (y < 10) {
        if (y >= 2) {
            break;
        }
        inner = inner + 1;
        y = y + 1;
    }
    x = x + 1;
}
print("Outer iterations:", outer);
print("Inner iterations:", inner);
