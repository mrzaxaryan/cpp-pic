{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "windows-i386-pic",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/loader.py",
            "args": [
                "--arch",
                "i386",
                "${workspaceFolder}/build/release/windows/i386/output.bin"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "windows-i386-release"
        },
        {
            "name": "windows-x86_64-pic",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/loader.py",
