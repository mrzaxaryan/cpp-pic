// Test for-each over arrays
var nums = [10, 20, 30, 40, 50];

// Compute sum with for-each: 10 + 20 + 30 + 40 + 50 = 150
var sum = 0;
for (var x in nums) {
    sum = sum + x;
}
var ok1 = sum == 150;

// Count iterations
var count = 0;
for (var x in nums) {
    count = count + 1;
}
var ok2 = count == 5;

// Verify mixed types iteration
var mixed = [1, "two", true, nil];
var typeCount = 0;
for (var item in mixed) {
    typeCount = typeCount + 1;
}
var ok3 = typeCount == 4;

var result = ok1 && ok2 && ok3;
