{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "i386-windows-release (PIC)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/loader.py",
            "args": [
                "--arch",
                "i386",
                "${workspaceFolder}/build/release/windows/i386/output.bin"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "i386-windows-release"
        },
        {
            "name": "x86_64-windows-release (PIC)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/loader.py",
            "args": [
                "--arch",
                "x86_64",
                "${workspaceFolder}/build/release/windows/x86_64/output.bin"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "x86_64-windows-release"
        },
        {
            "name": "aarch64-windows-release (PIC)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/loader.py",
            "args": [
                "--arch",
                "aarch64",
                "${workspaceFolder}/build/release/windows/aarch64/output.bin"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "aarch64-windows-release"
        },
        {
            "name": "i386-windows-debug",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/debug/windows/i386/output.exe",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "preLaunchTask": "i386-windows-debug"
        },
        {
            "name": "x86_64-windows-debug",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/debug/windows/x86_64/output.exe",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "preLaunchTask": "x86_64-windows-debug"
        },
        {
            "name": "aarch64-windows-debug",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/debug/windows/aarch64/output.exe",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "preLaunchTask": "aarch64-windows-debug"
        },
        {
            "name": "i386-linux-release (PIC)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/loader.py",
            "args": [
                "--arch",
                "i386",
                "${workspaceFolder}/build/release/linux/i386/output.bin"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "i386-linux-release"
        },
        {
            "name": "x86_64-linux-release (PIC)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/loader.py",
            "args": [
                "--arch",
                "x86_64",
                "${workspaceFolder}/build/release/linux/x86_64/output.bin"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "x86_64-linux-release"
        },
        {
            "name": "armv7a-linux-release (PIC)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/loader.py",
            "args": [
                "--arch",
                "armv7a",
                "${workspaceFolder}/build/release/linux/armv7a/output.bin"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "armv7a-linux-release"
        },
        {
            "name": "aarch64-linux-release (PIC)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/loader.py",
            "args": [
                "--arch",
                "aarch64",
                "${workspaceFolder}/build/release/linux/aarch64/output.bin"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "aarch64-linux-release"
        },
        {
            "name": "i386-linux-debug (WSL)",
            "type": "lldb-dap",
            "request": "launch",
            "program": "${workspaceFolder}/build/debug/linux/i386/output.elf",
            "cwd": "${workspaceFolder}",
            "internalConsoleOptions": "openOnSessionStart",
            "preLaunchTask": "i386-linux-debug"
        },
        {
            "name": "x86_64-linux-debug (WSL)",
            "type": "lldb-dap",
            "request": "launch",
            "program": "${workspaceFolder}/build/debug/linux/x86_64/output.elf",
            "cwd": "${workspaceFolder}",
            "internalConsoleOptions": "openOnSessionStart",
            "preLaunchTask": "x86_64-linux-debug"
        },
        {
            "name": "armv7a-linux-debug (WSL)",
            "type": "lldb-dap",
            "request": "launch",
            "program": "${workspaceFolder}/build/debug/linux/armv7a/output.elf",
            "cwd": "${workspaceFolder}",
            "internalConsoleOptions": "openOnSessionStart",
            "preLaunchTask": "armv7a-linux-debug"
        },
        {
            "name": "aarch64-linux-debug (WSL)",
            "type": "lldb-dap",
            "request": "launch",
            "program": "${workspaceFolder}/build/debug/linux/aarch64/output.elf",
            "cwd": "${workspaceFolder}",
            "internalConsoleOptions": "openOnSessionStart",
            "preLaunchTask": "aarch64-linux-debug"
        }
    ]
}