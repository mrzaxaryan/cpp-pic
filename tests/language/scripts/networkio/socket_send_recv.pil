// Test socket send and receive
var sock = sock_connect("httpbin.org", 80);
var req = "GET / HTTP/1.0\r\nHost: httpbin.org\r\n\r\n";
var sent = sock_send(sock, req);
print("Sent:", sent);
var data = sock_recv(sock, 100);
print("Received:", len(data), "bytes");
if (len(data) > 0) { print("PASS"); }
sock_close(sock);
