{
  "version": "2.0.0",
  "options": {
    "cwd": "${workspaceFolder}"
  },
  "windows": {
    "options": {
      "shell": {
        "executable": "cmd.exe",
        "args": [
          "/d",
          "/c"
        ]
      }
    }
  },
  "linux": {
    "options": {
      "shell": {
        "executable": "bash",
        "args": [
          "-c"
        ]
      }
    }
  },
  "presentation": {
    "echo": true,
    "reveal": "always",
    "focus": false,
    "panel": "shared",
    "showReuseMessage": false,
    "clear": true
  },
  "tasks": [
    {
      "label": "i386-windows-debug",
      "type": "shell",
      "command": "cmake -B build/debug/windows/i386/cmake -DARCHITECTURE=i386 -DPLATFORM=windows -DBUILD_TYPE=debug -DOPTIMIZATION_LEVEL=Og -G Ninja && cmake --build build/debug/windows/i386/cmake",
      "problemMatcher": [
        "$gcc"
      ],
      "group": "build"
    },
    {
